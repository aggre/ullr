{
	"name": "@aggre/ullr",
	"version": "1.0.3",
	"description": "Functional Web Components",
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"types": "./dist/ullr.d.ts",
	"files": [
		"dist/*.mjs",
		"dist/*.js",
		"dist/*.ts",
		"!**/*.test.*",
		"!**/test.js"
	],
	"scripts": {
		"test": "karma start && mocha dist/test.js --require ./node_modules/browser-env/register.js",
		"pretest": "yarn build",
		"build": "yarn build:ts && yarn build:rollup",
		"build:ts": "tsc -p ./",
		"build:rollup": "rollup -c",
		"prebuild": "yarn lint && yarn clean",
		"clean": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts --fix",
		"lint:format": "prettier --write '**/*.{ts,json,md,yml}'",
		"prepack": "yarn build"
	},
	"author": "aggre",
	"contributors": [
		"aggre <hiroyuki.aggre@gmail.com> (https://github.com/aggre)"
	],
	"license": "MIT",
	"dependencies": {
		"ramda": "^0.27.1"
	},
	"peerDependencies": {
		"lit-html": "^1.3.0",
		"rxjs": "^6.6.3 || ^6.6.3 || ^6.6.3 || ^7.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "19.0.0",
		"@rollup/plugin-multi-entry": "4.0.0",
		"@rollup/plugin-node-resolve": "13.0.0",
		"@rollup/plugin-typescript": "8.2.1",
		"@types/mocha": "8.2.2",
		"@types/node": "14.14.44",
		"@types/ramda": "0.27.40",
		"@typescript-eslint/eslint-plugin": "4.22.1",
		"@typescript-eslint/parser": "4.22.1",
		"assertthat": "5.2.8",
		"browser-env": "3.3.0",
		"eslint": "7.26.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-config-xo": "0.36.0",
		"eslint-config-xo-typescript": "0.41.1",
		"karma": "6.3.2",
		"karma-chrome-launcher": "3.1.0",
		"karma-mocha": "2.0.1",
		"karma-mocha-reporter": "2.2.5",
		"lit-html": "1.4.1",
		"mocha": "8.4.0",
		"prettier": "2.2.1",
		"puppeteer": "9.1.1",
		"rimraf": "3.0.2",
		"rollup": "2.47.0",
		"rollup-plugin-dts": "3.0.1",
		"rxjs": "7.0.0",
		"typescript": "4.2.4",
		"typescript-lit-html-plugin": "0.9.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aggre/ullr.git"
	},
	"bugs": {
		"url": "https://github.com/aggre/ullr/issues"
	},
	"homepage": "https://github.com/aggre/ullr#readme"
}

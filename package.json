{
	"name": "@aggre/ullr",
	"version": "0.15.0",
	"description": "Functional Web Components",
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"types": "./dist/ullr.d.ts",
	"files": [
		"dist/*.mjs",
		"dist/*.js",
		"dist/*.ts",
		"!**/*.test.*",
		"!**/test.js"
	],
	"scripts": {
		"test": "karma start && mocha dist/test.js --require ./node_modules/browser-env/register.js",
		"pretest": "npm run build",
		"build": "npm run build:ts && npm run build:rollup",
		"build:ts": "tsc -p ./",
		"build:rollup": "rollup -c",
		"prebuild": "npm run lint && npm run clean",
		"clean": "rimraf dist",
		"lint": "npm run lint:eslint && npm run lint:format",
		"lint:eslint": "eslint . --ext .ts --fix",
		"lint:format": "prettier --write '**/*.{ts,json,md,yml}'",
		"prepack": "npm run build"
	},
	"author": "aggre",
	"contributors": [
		"aggre <hiroyuki.aggre@gmail.com> (https://github.com/aggre)"
	],
	"license": "MIT",
	"dependencies": {
		"lit-html": "^1.3.0",
		"ramda": "^0.27.1",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.3"
	},
	"peerDependencies": {
		"lit-html": "^1.3.0",
		"rxjs": "^6.6.3"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "17.1.0",
		"@rollup/plugin-multi-entry": "4.0.0",
		"@rollup/plugin-node-resolve": "11.1.1",
		"@rollup/plugin-typescript": "8.1.1",
		"@types/mocha": "8.2.0",
		"@types/node": "14.14.25",
		"@types/ramda": "0.27.36",
		"@typescript-eslint/eslint-plugin": "4.14.2",
		"@typescript-eslint/parser": "4.14.2",
		"assertthat": "5.2.5",
		"browser-env": "3.3.0",
		"eslint": "7.19.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-config-xo": "0.34.0",
		"eslint-config-xo-typescript": "0.36.0",
		"karma": "5.2.3",
		"karma-chrome-launcher": "3.1.0",
		"karma-mocha": "2.0.1",
		"karma-mocha-reporter": "2.2.5",
		"mocha": "8.2.1",
		"prettier": "2.2.1",
		"puppeteer": "5.5.0",
		"rollup": "2.38.5",
		"rollup-plugin-dts": "2.0.1",
		"typescript": "4.1.3",
		"typescript-lit-html-plugin": "0.9.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aggre/ullr.git"
	},
	"bugs": {
		"url": "https://github.com/aggre/ullr/issues"
	},
	"homepage": "https://github.com/aggre/ullr#readme",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	}
}

{
	"name": "@aggre/ullr",
	"version": "0.15.0",
	"description": "Functional Web Components",
	"main": "index.js",
	"files": [
		"directive/",
		"lib/",
		"src/",
		"!**/*.test.*",
		"index.d.ts"
	],
	"scripts": {
		"test": "karma start && mocha dist/test.js --require ./node_modules/browser-env/register.js",
		"pretest": "npm run build && rollup -c",
		"build": "tsc -p ./",
		"prebuild": "npm run lint",
		"lint": "npm run lint:eslint && npm run lint:format",
		"lint:eslint": "eslint . --ext .ts --fix",
		"lint:format": "prettier --write '**/*.{ts,json,md,yml}'",
		"prepare": "npm run build"
	},
	"author": "aggre",
	"contributors": [
		"aggre <hiroyuki.aggre@gmail.com> (https://github.com/aggre)"
	],
	"license": "MIT",
	"dependencies": {
		"lit-html": "^1.3.0",
		"ramda": "^0.27.1",
		"rxjs": "^6.6.3"
	},
	"peerDependencies": {
		"lit-html": "^1.3.0",
		"rxjs": "^6.6.3"
	},
	"devDependencies": {
		"@types/mocha": "8.0.4",
		"@types/node": "14.14.7",
		"@types/ramda": "0.27.32",
		"@typescript-eslint/eslint-plugin": "4.7.0",
		"@typescript-eslint/parser": "4.7.0",
		"assertthat": "5.2.5",
		"browser-env": "3.3.0",
		"eslint": "7.13.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-config-xo": "0.33.1",
		"eslint-config-xo-typescript": "0.35.0",
		"karma": "5.2.3",
		"karma-chrome-launcher": "3.1.0",
		"karma-mocha": "2.0.1",
		"karma-mocha-reporter": "2.2.5",
		"mocha": "8.2.1",
		"prettier": "2.1.2",
		"puppeteer": "5.4.1",
		"rollup": "2.33.2",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-multi-entry": "2.1.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-typescript2": "0.29.0",
		"typescript": "4.0.5",
		"typescript-lit-html-plugin": "0.9.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aggre/ullr.git"
	},
	"bugs": {
		"url": "https://github.com/aggre/ullr/issues"
	},
	"homepage": "https://github.com/aggre/ullr#readme",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	}
}
